{% extends 'base.html.twig' %}

{% block body %}

    <h2>Liste des campus</h2>

    <ul>
        {% for campus in campus %}
            <br>
            <li>Campus: {{ campus.nom }}</li>
            <a href="{{ path('campus_update', {'id':campus.id}) }}">Modifier</a> | <a href="{{ path('campus_delete', {'id': campus.id}) }}">Supprimer</a>
            <br>
        {% endfor %}
    </ul>

    <a href="{{ path('campus_add') }}">Ajouter un campus</a>
{% endblock %}

{% block title %}

{% endblock %}

